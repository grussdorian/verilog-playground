S_0x128604fd0 .scope module, "tb_Dff" "tb_Dff" 2 1;
 .timescale 0 0;
P_0x128604d00 .param/l "clock_period" 0 2 2, +C4<00000000000000000000000000000101>;
P_0x128604d40 .param/l "end_time" 0 2 3, +C4<00000000000000000000000001100100>;
v0x149406dd0_0 .var "Clk", 0 0;
v0x149406e90_0 .var "D", 0 0;
v0x149406f40_0 .net "Q", 0 0, v0x149406c20_0;  1 drivers
v0x149407010_0 .var "R", 1 0;
v0x1494070a0_0 .var "Rst", 0 0;
E_0x128604dc0 .event negedge, v0x1286053a0_0;
S_0x128605230 .scope module, "dff" "udpDff" 2 5, 3 1 0, S_0x128604fd0;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "Q";
    .port_info 1 /INPUT 1 "D";
    .port_info 2 /INPUT 1 "Clk";
    .port_info 3 /INPUT 1 "Rst";
v0x1286053a0_0 .net "Clk", 0 0, v0x149406dd0_0;  1 drivers
v0x149406b70_0 .net "D", 0 0, v0x149406e90_0;  1 drivers
v0x149406c20_0 .var "Q", 0 0;
v0x149406cd0_0 .net "Rst", 0 0, v0x1494070a0_0;  1 drivers
E_0x1286051f0 .event posedge, v0x149406cd0_0, v0x1286053a0_0;
    .scope S_0x128605230;
T_0 ;
    %wait E_0x1286051f0;
    %load/vec4 v0x149406cd0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x149406b70_0;
    %store/vec4 v0x149406c20_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x149406c20_0, 0, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x128604fd0;
T_1 ;
    %vpi_call 2 9 "$monitor", $time, " Q=%b, D=%d, Clk=%d, Rst=%b", v0x149406f40_0, v0x149406e90_0, v0x149406dd0_0, v0x1494070a0_0 {0 0 0};
    %vpi_call 2 11 "$dumpfile", "VCD/Dff.vcd" {0 0 0};
    %vpi_call 2 12 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x128604fd0 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x128604fd0;
T_2 ;
    %wait E_0x128604dc0;
    %delay 2, 0;
    %vpi_func 2 29 "$random" 32 {0 0 0};
    %pushi/vec4 2, 0, 32;
    %mod/s;
    %pad/s 2;
    %store/vec4 v0x149407010_0, 0, 2;
    %load/vec4 v0x149407010_0;
    %parti/s 1, 0, 2;
    %store/vec4 v0x149406e90_0, 0, 1;
    %load/vec4 v0x149407010_0;
    %parti/s 1, 1, 2;
    %store/vec4 v0x1494070a0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x128604fd0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x149406dd0_0, 0, 1;
    %end;
    .thread T_3;
    .scope S_0x128604fd0;
T_4 ;
    %delay 5, 0;
    %load/vec4 v0x149406dd0_0;
    %inv;
    %store/vec4 v0x149406dd0_0, 0, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x128604fd0;
T_5 ;
    %delay 100, 0;
    %vpi_call 2 37 "$finish" {0 0 0};
    %end;
    .thread T_5;
